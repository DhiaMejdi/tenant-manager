    <property name="styleSheet">
     <string notr="true">/* General QWidget styling with animated background */
QWidget {
    background: linear-gradient(45deg, #6A4CFC, #9C6EFC);
    background-size: 400% 400%; /* Large background size for animation */
    animation: gradientAnimation 15s ease infinite; /* Apply the animation */
    color: #FFF; /* White text */
    border-radius: 15px; /* Rounded corners */
    padding: 20px;
    font-family: 'Roboto', sans-serif;
    font-size: 14px;
    transition: all 0.3s ease-in-out; /* Smooth transition for hover effects */
}

/* Background gradient animation keyframes */
@keyframes gradientAnimation {
    0% {
        background-position: 0% 50%; /* Start from the left */
    }
    50% {
        background-position: 100% 50%; /* Move to the right */
    }
    100% {
        background-position: 0% 50%; /* Return to the start */
    }
}

/* Hover effect for QWidget */
QWidget:hover {
    background: #5A2D84; /* Lighter purple when hovering */
}

/* QLabel Styling */
QLabel {
    color: #E2B0FF; /* Light purple text for a futuristic feel */
    font-size: 16px;
    font-weight: bold;
    margin-bottom: 15px;
    text-shadow: 0 0 8px rgba(226, 176, 255, 0.6); /* Soft purple glow */
}

/* QPushButton with smooth hover effect */
QPushButton {
    background: linear-gradient(45deg, #6A4CFC, #9C6EFC); /* Purple gradient button */
    border: 2px solid #9C6EFC; /* Soft border with light purple */
    color: #FFF; /* White text */
    border-radius: 12px;
    padding: 12px 30px;
    font-size: 16px;
    font-weight: bold;
    text-transform: uppercase;
    transition: all 0.3s ease-in-out;
    box-shadow: 0 0 15px rgba(106, 76, 252, 0.6); /* Subtle glowing effect */
}

/* Hover effect for QPushButton */
QPushButton:hover {
    background: linear-gradient(45deg, #9C6EFC, #6A4CFC); /* Reverse gradient for hover */
    border-color: #6A4CFC;
    box-shadow: 0 0 20px rgba(106, 76, 252, 0.8); /* Stronger glow on hover */
    transform: scale(1.05); /* Slight scale-up for hover effect */
}

/* Pressed effect for QPushButton */
QPushButton:pressed {
    background: #6A4CFC; /* Dark purple when pressed */
    box-shadow: 0 0 10px rgba(106, 76, 252, 0.8); /* Still glowing effect */
    transform: scale(0.98); /* Slight scale-down */
}

/* QLineEdit with purple border focus */
QLineEdit {
    background-color: #3A1D6A; /* Dark purple background */
    border: 2px solid #6A4CFC;
    color: #FFF; /* White text */
    border-radius: 8px;
    padding: 12px;
    font-size: 14px;
    transition: all 0.3s ease;
}

QLineEdit:focus {
    border-color: #9C6EFC; /* Lighter purple when focused */
    box-shadow: 0 0 10px rgba(156, 110, 252, 0.7); /* Soft purple glow */
}

/* QComboBox */
QComboBox {
    background-color: #3A1D6A; /* Dark purple background */
    border: 2px solid #6A4CFC;
    color: #FFF;
    border-radius: 8px;
    padding: 6px;
    font-size: 14px;
    transition: all 0.3s ease;
}

QComboBox:hover {
    border-color: #9C6EFC; /* Lighter border color on hover */
}

QComboBox:focus {
    border-color: #9C6EFC;
    box-shadow: 0 0 10px rgba(156, 110, 252, 0.7); /* Focus effect with purple glow */
}

/* QTableView with purple and neon accents */


QHeaderView::section {
    background-color: #6A4CFC; /* Purple header background */
    color: #FFF;
    font-size: 16px;
    font-weight: bold;
    padding: 10px;
    border: 1px solid #6A4CFC; /* Dark purple border for headers */
    border-radius: 5px;
    text-align: center;
}
/* Hover effect for table rows */
QTableView::item:hover {
    background-color: #9C6EFC; /* Lighter purple when hovering over a row */
    color: #181818; /* Darker text when hovered */
}



/* Scrollbar design with a futuristic purple look */
QScrollBar:vertical, QScrollBar:horizontal {
    background: #2E1A47; /* Dark background for scrollbar */
    width: 12px;
    border-radius: 8px;
}

QScrollBar::handle:vertical, QScrollBar::handle:horizontal {
    background: #6A4CFC; /* Purple scrollbar handle */
    border-radius: 6px;
}

QScrollBar::handle:vertical:hover, QScrollBar::handle:horizontal:hover {
    background: #9C6EFC; /* Lighter purple on hover */
}

/* QLabel for displaying the QR code or any other label */
QLabel {
    color: #E2B0FF; /* Light purple text */
    font-size: 18px;
    font-weight: bold;
    text-shadow: 0 0 10px rgba(226, 176, 255, 0.6); /* Glowing effect */
}
QTableView {
    background-color: #2E1A47; /* Dark background for the table */
    border: 2px solid #6A4CFC; /* Dark purple border around the table */
    color: #FFF; /* White text */
    font-size: 14px;
    font-family: 'Roboto', sans-serif;
    border-radius: 10px;
    alternate-background-color: #3A1D6A; /* Slightly lighter alternate rows */
}</string>
