       <string notr="true">/* General QComboBox styling */
QComboBox {
    background-color: #5E35B1;  /* Dark purple background */
    color: white;  /* White text color */
    border: 1px solid #8E24AA;  /* Lighter purple border */
    border-radius: 12px;  /* Rounded corners */
    padding: 8px;  /* Padding inside the combo box */
    font-size: 16px;  /* Set the font size */
}

/* QComboBox when it is focused (when clicked or selected) */
QComboBox:focus {
    border: 1px solid #9C27B0;  /* Purple color when focused */
    background-color: #7E57C2;  /* Lighter purple background when focused */
}

/* QComboBox drop-down button (the arrow) */
QComboBox::drop-down {
    border: none;  /* Remove the border */
    background-color: transparent;  /* Transparent background */
    width: 30px;  /* Set width for the drop-down button */
}

/* QComboBox button when hovered */
QComboBox::drop-down:hover {
    background-color: #AB47BC;  /* Slightly brighter purple for hover effect */
}

/* The combo box popup (the list) */
QListView {
    background-color: #7E57C2;  /* Lighter purple for the dropdown list */
    color: white;  /* White text in the list */
    border-radius: 8px;  /* Rounded corners for the dropdown list */
}

/* When an item in the dropdown list is hovered */
QListView::item:hover {
    background-color: #9C27B0;  /* Lighter purple for hovered item */
    border-radius: 8px;  /* Rounded corners for hovered item */
}

/* Style for the selected item in the dropdown list */
QListView::item:selected {
    background-color: #AB47BC;  /* Even brighter purple for selected item */
}

/* Disabled state for the combo box */
QComboBox:disabled {
    background-color: #B39DDB;  /* Lighter background when disabled */
    border: 1px solid #D1C4E9;  /* Light border when disabled */
    color: #9E9E9E;  /* Light gray text when disabled */
}

/* Optional: smooth transition for combo box focus effect */
QComboBox {
    transition: background-color 0.3s ease, border-color 0.3s ease;
}
